{% extends 'base.html.twig' %}

{% block title %}Season{% endblock %}

{% block body %}
    <h1>{{ season.description }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Program</th>
                <td>{{ program.title }}</td>
            </tr>

            <tr>
                <th>Year</th>
